{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "./",
            "args": ["offline", "-file", "C:/Git/my.mojo.monorepo/api/applications-api/terraform/api.tf", "-module", "applications_api"]
        },
        {
            "name": "Attach Node",
            "type": "node",
            "request": "attach",
            "port": 9229,
            "restart": true,
            "continueOnAttach": true,
            "address": "localhost",
            "localRoot": "${workspaceFolder}",
            "remoteRoot": "${workspaceFolder}",
            "outFiles": ["${workspaceFolder}/.terrable/**/*.js"],
            "sourceMaps": true,
            "resolveSourceMapLocations": [
                "${workspaceFolder}/.terrable/**/*.js.map",
            ],
            "skipFiles": ["<node_internals>/**"]
        },
    ],
    "compounds": [
        {
            "name": "Go + Node",
            "configurations": ["Launch Package", "Attach Node"]
        }
    ]
}